[color]
        ui = true
[merge]
        ff = false
[alias]
        co = checkout
        ci = commit
        st = status
        br = branch
        tree = log --graph --abbrev-commit --decorate --all
[color]
        branch = auto
        diff = auto
        status = auto
[color "branch"]
        current = magenta
        local = yellow
        remote = cyan
[color "diff"]
        meta = yellow
        frag = magenta
        old = red
        new = green
[color "status"]
        added = yellow
        changed = green
        untracked = cyan
[core]
        excludesfile = /Users/deac/.gitignore_global
        ignorecase = false
[push]
        default = simple
[user]
	name = Deac Karns
	email = deac.karns@dtn.com

[difftool "smerge"]
        cmd = smerge \"$BASE\" \"$LOCAL\" \"$REMOTE\"
[diff]
        tool = smerge
[mergetool "smerge"]
        cmd = smerge mergetool \"$BASE\" \"$LOCAL\" \"$REMOTE\" -o \"$MERGED\"
        trustExitCode = true
